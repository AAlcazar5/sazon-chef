{
  "name": "sazon-backend",
  "version": "1.0.0",
  "description": "Sazon Chef Backend API",
  "main": "dist/server.js",
  "prisma": {
    "seed": "ts-node -r tsconfig-paths/register prisma/seed.ts"
  },
  "scripts": {
    "dev": "ts-node -r tsconfig-paths/register src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "seed": "ts-node -r tsconfig-paths/register prisma/seed.ts",
    "seed:clear": "CLEAR_DB=true ts-node -r tsconfig-paths/register prisma/seed.ts",
    "seed:local": "ts-node -r tsconfig-paths/register scripts/seed-local-recipes.ts",
    "seed:ai": "ts-node -r tsconfig-paths/register scripts/seed-ai-recipes.ts",
    "seed:100": "ts-node -r tsconfig-paths/register scripts/seed-100-recipes.ts",
    "seed:1000": "ts-node -r tsconfig-paths/register scripts/seed-1000-recipes.ts",
    "seed:100-comprehensive": "ts-node -r tsconfig-paths/register scripts/seed-100-comprehensive.ts",
    "seed:500-comprehensive": "ts-node -r tsconfig-paths/register scripts/seed-500-comprehensive.ts",
    "seed:comprehensive": "ts-node -r tsconfig-paths/register scripts/seed-comprehensive-recipes.ts",
    "seed:quick-meals": "ts-node -r tsconfig-paths/register scripts/seed-quick-meals.ts",
    "check:quick-meals": "ts-node -r tsconfig-paths/register scripts/check-quick-meals.ts",
    "monitor:seeding": "ts-node -r tsconfig-paths/register scripts/monitor-seeding.ts",
    "topup:1000": "ts-node -r tsconfig-paths/register scripts/top-up-to-1000.ts",
    "test:performance": "ts-node -r tsconfig-paths/register scripts/test-performance.ts",
    "backup:create": "ts-node -r tsconfig-paths/register scripts/backupRecipes.ts backup",
    "backup:restore": "ts-node -r tsconfig-paths/register scripts/backupRecipes.ts restore",
    "update:images": "ts-node -r tsconfig-paths/register scripts/update-recipe-images.ts",
    "refresh:images": "ts-node -r tsconfig-paths/register scripts/refresh-all-images.ts",
    "check:images": "ts-node -r tsconfig-paths/register scripts/check-image-urls.ts",
    "ensure:unique-images": "ts-node -r tsconfig-paths/register scripts/ensure-unique-images.ts",
    "test:unique-images": "ts-node -r tsconfig-paths/register scripts/test-unique-images.ts",
    "fetch-recipes": "ts-node -r tsconfig-paths/register scripts/fetch-recipes-simple.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testPathPattern=integration"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.68.0",
    "@google/generative-ai": "^0.24.1",
    "@prisma/client": "^5.22.0",
    "@types/multer": "^2.0.0",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.17",
    "@types/uuid": "^10.0.0",
    "axios": "^1.13.0",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.3.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "openai": "^6.7.0",
    "passport": "^0.7.0",
    "passport-apple": "^2.0.2",
    "passport-google-oauth20": "^2.0.0",
    "prisma": "^5.7.1",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/node": "^20.19.23",
    "@types/supertest": "^2.0.16",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  }
}
